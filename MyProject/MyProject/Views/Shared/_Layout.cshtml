
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MyProject</title>
    
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link href="~/css/AddItem.css" type="text/css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/remixicon@3.4.0/fonts/remixicon.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Great+Vibes&family=Lugrasimo&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Barlow:wght@600&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Noto+Sans&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Barlow:wght@300&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@300&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@200&display=swap" rel="stylesheet">
    <link href="~/_Layout.cshtml.css" type="text/css" rel="stylesheet" />
</head>
<body>
    <header>
        <a href="/MainPage" style="cursor:pointer">
            <figure>
                <img src="https://w7.pngwing.com/pngs/188/439/png-transparent-attack-on-titan-recon-corps-logo-a-o-t-wings-of-freedom-eren-yeager-attack-on-titan-vol-9-logo-freedom-angle-cartoon-crunchyroll.png" />
                <figcaption>
                    <p>Photovibe</p>
                    <p>Good for your satisfying</p>
                </figcaption>
            </figure>
        </a>
        @await Component.InvokeAsync("Category")
    </header>
    @RenderBody()
    <script>
       
        $(window).scroll(function () {
            if ($(this).scrollTop() > 250) {
                $('header').fadeOut();
            } else {
                $('header').fadeIn();
            }
        });

    </script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
   
    @await RenderSectionAsync("Scripts", required: false)
   
</body>
</html>
